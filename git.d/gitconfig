[alias]
	ci = commit
	cit = commit -a -m
	co = checkout
	l = log --oneline --decorate
	st = status
	unstage = reset HEAD --
	graph = log --color --graph --pretty='format:%x09%h <%cn> %ci %s'
[color]
	ui = auto
[core]
	autocrlf = input
[push]
	default = simple
[branch]
	autosetuprebase = always
[include]
	path = .gitconfig.local
[interactive]
	singlekey = true
